pico-8 cartridge // http://www.pico-8.com
version 8
__lua__
-- dominique
-- by libby

hearts = "‡‡‡‡"

t=0
n=0

scene = 1

x=64
y=64

-- reset
x = 20; y = 60

--music(0)

function _init()
 init_scene(scenes[scene])
end

function _update()
 t += 1
 update_scene(scenes[scene])
end

function _draw()
 cls()
 
 --map(0,0,0,0,16,16)
 draw_scene(scenes[scene])
  
 draw_dashboard()
end

------------------------------
--         dashboard        --
------------------------------

function draw_dashboard()
  for i=0,3 do
    spr(0,i*8+3,2)
  end
  
  print(hearts,3,3,8)
  print("day 1",100,3,8)
end

-------------------------------
--       characters          --
-------------------------------

function draw_lg(s,x,y)
  cn = characters[s]
  spr(cn,x,y)
  spr(cn+1,x+8,y)
  spr(cn+16,x,y+8)
  spr(cn+17,x+8,y+8)
end

characters = 
 { dominique=45,
   rosalina=13 }
 
function draw_queen(x,y)
  cn = 97
  spr(cn,x,y)
  spr(cn+1,x+8,y)
  spr(cn+2,x+16,y)
  spr(cn+3,x+24,y)
  spr(cn+4,x+32,y)
  spr(cn+5,x+40,y)
  spr(cn+16,x,y+8)
  spr(cn+17,x+8,y+8)
  spr(cn+18,x+16,y+8)
  spr(cn+19,x+24,y+8)
  spr(cn+20,x+32,y+8)
  spr(cn+21,x+40,y+8)
end

------------------------------
--          scenes          --
------------------------------

function new_scene(s)
  s = s or {}
  s.title = ""
  s.curr_dia = 0
  s.dia_mode = 1
  s.dialog = { 1,2 }
  s.map = 2
  s.init = init_scene
  s.draw = draw_seene
  s.update = update_scene
  return s
end

function init_scene(s)
  s.curr_dia=s.dialog[1]
end

function update_scene(s)
 if s.dia_mode == 1
 then update_dia(s)
 else dir4_handler()
 end
end

function draw_scene(s)
  map(16,16,0,0,16,16)

  draw_queen(40,20)

  draw_lg("dominique",x,y)
  draw_lg("rosalina",90,60)
 
  if s.dia_mode == 1
  then
    draw_dia(s)
  end
end

scenes = {
  new_scene()
  }

------------------------------
--          dialog          --
------------------------------

function tgl_btn(d)
  if d.selected == 2
  then d.selected = 1
  else d.selected = 2
  end
end

function next_dia(s)
  reset_dia(s)
  if s.curr_dia < #dialog
  then s.curr_dia += 1
  else s.dia_mode = 0
  end
end

function reset_dia(s)
  d = dialog[s.curr_dia]
  o.n = 1
  o.selected = 1
  d.c1 = ""
  d.c2 = ""
end

function update_dia(s)
  d = dialog[s.curr_dia]
  
  d.n += 1
  local n2 = 0
  if (d.n > #d.w1) n2 = d.n - #d.w1 

  d.c1 = sub(d.w1,1,d.n)
  d.c2 = sub(d.w2,1,n2)
  dialog[s.curr_dia] = d
  
  if ready(d,15)
  then
   if (btnp(0)) tgl_btn(d)
   if (btnp(1)) tgl_btn(d)
   if (btnp(2)) tgl_btn(d)
   if (btnp(3)) tgl_btn(d)
 
   if (btnp(5)) next_dia(s)
  end
end

function draw_dia(s)
  o = dialog[s.curr_dia]
  rectfill(0,90,128,128,7)
  rect(1,91,126,126,8)
  spr(73,5,95)
  spr(74,13,95)
  spr(89,5,103)
  spr(90,13,103)
  
  print(o.c1,24,96,12)
  print(o.c2,24,104,12)
  print(o.c3,24,112,12)
  
  if (#o.btns > 0 and ready(d,15))
  then draw_btns(o)
  else print(o.c3,24,112,120)
  end
end

function ready(o,pause)
  return o.n > #o.w1 + #o.w2 + pause
end

function draw_btns(d)
  b1 = d.btns[1]
  b2 = d.btns[2]
  rectfill(22,114,60,122,8)
  rectfill(70,114,110,122,8)
  print(b1,24,116,7)
  print(b2,72,116,7)
  bx = 70
  if (d.selected == 1) bx = 22
  rect(bx,114,bx+40,122,7)
  spr(3+slow(10),bx+20,108)   
end

-- dialog constructor
function md(w1,w2,btns)
  tmp_dia = {}
  tmp_dia.selected = 1
  tmp_dia.n = 1
  tmp_dia.w1 = w1
  tmp_dia.w2 = w2
  tmp_dia.w3 = ""
  tmp_dia.c1 = ""
  tmp_dia.c2 = ""
  tmp_dia.c3 = ""
  tmp_dia.btns = btns
  return tmp_dia
end

dialog = 
 { md("are you ready to compete",
      "to become my successor?",
      {"yes!", "i dunno..."}),
   md("what will be your ",
      "strategy?",
      {"crafty", "fight!"}) }

-------------------------------
--        movement           --
-------------------------------

function dir4_handler()
 if (btn(0)) x -= 1
 if (btn(1)) x += 1
 if (btn(2)) y -= 1
 if (btn(3)) y += 1
end

function dir2_handler()
 if (btn(0)) x -= 1
 if (btn(1)) x += 1
end
       
-------------------------------
--          helpers          --
-------------------------------

-- slow time :p
function slow(f)
  return (flr(t/f))%2
end
