pico-8 cartridge // http://www.pico-8.com
version 8
__lua__
-- dominique
-- by libby

curr_dia = 1
dia_btn = 1
dia_mode = 1

hearts = "‡‡‡‡"

t=0
n=0

scene = 1

x=64
y=64

-- reset
x = 20; y = 60

--music(0)

function _init()
 init_scene(scenes[scene])
end

function _update()
 t += 1
 if dia_mode == 1
 then update_dia()
 else dir4_handler()
 end
end

function _draw()
 cls()
 
 --map(0,0,0,0,16,16)
 draw_scene(scenes[scene])
  
 if dia_mode == 1
 then
   draw_dia()
 end
  
 draw_dashboard()
end

------------------------------
--         dashboard        --
------------------------------

function draw_dashboard()
  print(hearts,3,3,8)
  print("day 1",100,3,8)
end

-------------------------------
--       characters          --
-------------------------------

function draw_lg(s,x,y)
  cn = characters[s]
  spr(cn,x,y)
  spr(cn+1,x+8,y)
  spr(cn+16,x,y+8)
  spr(cn+17,x+8,y+8)
end

characters = 
 { dominique=65,
   rosalina=71 }
 
function draw_queen(x,y)
  cn = 97
  spr(cn,x,y)
  spr(cn+1,x+8,y)
  spr(cn+2,x+16,y)
  spr(cn+3,x+24,y)
  spr(cn+4,x+32,y)
  spr(cn+5,x+40,y)
  spr(cn+16,x,y+8)
  spr(cn+17,x+8,y+8)
  spr(cn+18,x+16,y+8)
  spr(cn+19,x+24,y+8)
  spr(cn+20,x+32,y+8)
  spr(cn+21,x+40,y+8)
end

------------------------------
--          scenes          --
------------------------------

function new_scene(s)
  s = s or {}
  s.title = ""
  s.dialog = { 1,2 }
  s.map = 2
  s.init = init_scene
  s.draw = draw_seene
  s.update = update_scene
  return s
end

function init_scene(s)
  curr_dia=s.dialog[1]
end

function update_scene(s)
  
end

function draw_scene(s)
 map(16,16,0,0,16,16)

 draw_queen(40,20)

 draw_lg("dominique",x,y)
 draw_lg("rosalina",90,60)
end

scenes = {
  new_scene()
  }

------------------------------
--          dialog          --
------------------------------

function tgl_btn()
  if dia_btn == 2
  then dia_btn = 1
  else dia_btn = 2
  end
end

function next_dia()
  n=1
  dia_btn=1
  reset_dia()
  if curr_dia < #dialog
  then curr_dia += 1
  else curr_dia = 1
  end
end

function reset_dia()
  d = dialog[curr_dia]
  d.c1 = ""
  d.c2 = ""
end

function update_dia()
  d = dialog[curr_dia]
  
  n += 1
  local n2 = 0
  local ready = n > #d.w1 + #d.w2 + 20
  
  if (n > #d.w1) n2 = n - #d.w1 

  d.c1 = sub(d.w1,1,n)
  d.c2 = sub(d.w2,1,n2)
  dialog[curr_dia] = d
  
  if ready 
  then
   if (btnp(0)) tgl_btn()
   if (btnp(1)) tgl_btn()
   if (btnp(2)) tgl_btn()
   if (btnp(3)) tgl_btn()
 
   if (btnp(5)) next_dia()
  end
end

function draw_dia()
  o = dialog[curr_dia]
  rectfill(0,90,128,128,7)
  rect(1,91,126,126,8)
  spr(73,5,95)
  spr(74,13,95)
  spr(89,5,103)
  spr(90,13,103)
  
  print(o.c1,24,96,12)
  print(o.c2,24,104,12)
  
  local ready = n > #o.w1 + #o.w2 + 10
  if (o.b1 != "" and o.b2 != "" and ready)
  then draw_btns(o.b1, o.b2)
  end
end

function draw_btns(w1,w2)
  rectfill(22,114,60,122,8)
  rectfill(70,114,110,122,8)
  print(w1,24,116,7)
  print(w2,72,116,7)
  bx = 70
  if (dia_btn == 1) bx = 22
  rect(bx,114,bx+40,122,7)
  spr(3+slow(10),bx+20,108)   
end

-- dialog constructor
function md(w1,w2,b1,b2)
  tmp_dia = {}
  tmp_dia.w1 = w1
  tmp_dia.w2 = w2
  tmp_dia.c1 = ""
  tmp_dia.c2 = ""
  tmp_dia.b1 = b1
  tmp_dia.b2 = b2
  return tmp_dia
end

dialog = 
 { md("are you ready to compete",
      "to become my successor?",
      "yes!", "i dunno..."),
   md("what will be your ",
      "strategy?",
      "crafty", "fight!") }

-------------------------------
--        movement           --
-------------------------------

function dir4_handler()
 if (btn(0)) x -= 1
 if (btn(1)) x += 1
 if (btn(2)) y -= 1
 if (btn(3)) y += 1
end

function dir2_handler()
 if (btn(0)) x -= 1
 if (btn(1)) x += 1
end
       
-------------------------------
--          helpers          --
-------------------------------

-- slow time :p
function slow(f)
  return (flr(t/f))%2
end
